{
  "name": "@calderaro/react-toolbox",
  "version": "0.0.34",
  "description": "react tool box",
  "author": "Angel Calderaro",
  "license": "ISC",
  "main": "dist/lib.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:calderaro/react-tool-box.git"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-app": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "rollup -c",
    "start-rollup": "rollup -c -w",
    "prepublish": "npm run build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --fix --color",
    "lint:ci": "eslint --ext .js,.jsx,.ts,.tsx src --quiet"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "react-scripts": "4.0.1",
    "csx": "^10.0.1",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rollup-plugin-url": "^3.0.1",
    "typescript": "~4.1.2",
    "typestyle": "^2.1.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/ramda": "^0.27.32",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@svgr/rollup": "^5.5.0",
    "@types/lodash": "^4.14.165",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "rollup": "^2.34.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-typescript2": "^0.29.0"
  }
}
